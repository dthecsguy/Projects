(echo A && echo B) || (echo C && ls -a) || (echo failed)
(echo hi) || (echo failed)
echo A || (ls -R && echo hi) || echo B || (echo failed)
(ps;ls) || (ls) || (echo failed)
(echo a && echo b || echo c) && echo d || (echo failed)
(ps && ls) || (echo A && echo failed)
(echo A && echo B) || (echo C && echo failed)
(echo hi there && ls -a) || (echo failed)
(mkdir testdir && echo created test directory) || (echo failed)
(ls && rm -r testdir) && echo removed test directory || (echo failed)
exit || (echo failed)
